<Page x:Class="VitAdmin.View.ViewProfessionnelDossierPatientInfosModif"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:VitAdmin.View"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="ViewProfessionnelDossierPatientInfosModif"
      Name="WinProPatientInfosModif">
    
      

    <Grid Name="grdPatientInfos">
        <Grid.RowDefinitions>
            <RowDefinition Height="0.2*"/>
            <RowDefinition/>
            <RowDefinition Height="0.2*"/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Grid.ColumnSpan="2" FontSize="24" Foreground="Gray">Modification des informations</Label>
        <Button Grid.Row="2" VerticalAlignment="Center" MaxWidth="100" Content="Modifier"
                Command="{Binding CmdBtnModif}">
            
        </Button>
    </Grid>
</Page>
